{
  "name": "irfan_m/e-report",
  "description": "E-Report with PHP OOP by Irfan M",
  "type": "project",
  "authors": [
    {
      "name": "irfan m",
      "email": "imachmud97@gmail.com"
    }
  ],
  "require": {
    "php": ">=8",
    "twbs/bootstrap": "v5.3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "10.3.5"
  },
  "autoload": {
    "psr-4": {
      "IRFANM\\PHP\\EREPORT\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IRFANM\\PHP\\EREPORT\\": "tests"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "rm -rf public/bootstrap",
      "cp -R vendor/twbs/bootstrap/dist public/bootstrap"
    ]
  }
}
